{
  "name": "lootcord",
  "version": "1.0.0",
  "description": "Collect items and battle your friends!",
  "main": "./src/index.js",
  "scripts": {
    "start": "node -r dotenv/config src/index.js",
    "dev": "nodemon -r dotenv/config src/index.js",
    "fix": "eslint --fix --ext .js .",
    "lint": "eslint --ext .js .",
    "test": "jest && npm run lint",
    "migrate": "db-migrate",
    "migrate:up": "db-migrate up",
    "migrate:down": "db-migrate down",
    "register-slash": "node scripts/register-slash-commands.js"
  },
  "author": "blobfysh#4679",
  "repository": {
    "type": "git",
    "url": "https://github.com/blobfysh/Lootcord.git"
  },
  "license": "GPL-3.0",
  "dependencies": {
    "axios": "^0.21.1",
    "bad-words": "^3.0.4",
    "big-time": "^2.0.1",
    "body-parser": "^1.19.0",
    "canvas": "^2.8.0",
    "cron": "^1.8.2",
    "db-migrate": "^0.11.12",
    "db-migrate-mysql": "^2.1.2",
    "dotenv": "^8.2.0",
    "emoji-regex": "^9.2.2",
    "eris": "github:abalabahaha/eris#dev",
    "eris-sharder": "github:discordware/eris-sharder#eris-dev",
    "express": "^4.17.1",
    "html-entities": "^2.3.2",
    "ioredis": "^4.27.3",
    "js-levenshtein": "^1.1.6",
    "mysql": "^2.18.1",
    "shortid": "^2.2.16",
    "slash-commands": "^1.4.0"
  },
  "devDependencies": {
    "eslint": "^7.24.0",
    "eslint-plugin-jest": "^24.3.5",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "scripts/*",
      "migrations/*"
    ],
    "delay": "2500"
  }
}
